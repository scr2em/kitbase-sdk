{
  "name": "@kitbase/changelogs-react",
  "version": "0.1.0",
  "description": "Kitbase Changelogs React SDK - React hooks and providers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
      "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" }
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": ["kitbase", "changelogs", "react", "hooks"],
  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/scr2em/kitbase-sdk.git", "directory": "changelogs/react" },
  "peerDependencies": { "react": ">=17.0.0" },
  "dependencies": { "@kitbase/changelogs": "workspace:*" },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.48",
    "jsdom": "^23.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": { "node": ">=18" }
}
