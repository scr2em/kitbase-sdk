{
  "name": "@kitbase/changelogs",
  "version": "0.1.0",
  "description": "Kitbase Changelogs SDK for TypeScript and JavaScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": ["kitbase", "changelogs", "release-notes"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scr2em/kitbase-sdk.git",
    "directory": "changelogs/typescript"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": { "node": ">=18" }
}
