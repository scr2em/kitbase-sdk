{
  "name": "@kitbase/events-react",
  "version": "0.1.0",
  "description": "Kitbase Events React SDK - React hooks and providers for event tracking",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "kitbase",
    "events",
    "react",
    "hooks",
    "analytics",
    "tracking"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scr2em/kitbase-sdk.git",
    "directory": "customEvents/react"
  },
  "bugs": {
    "url": "https://github.com/scr2em/kitbase-sdk/issues"
  },
  "homepage": "https://github.com/scr2em/kitbase-sdk/tree/main/customEvents/react#readme",
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@kitbase/events": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.48",
    "jsdom": "^23.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
