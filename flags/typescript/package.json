{
  "name": "@kitbase/flags",
  "version": "0.1.0",
  "description": "Kitbase Feature Flags SDK for TypeScript and JavaScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
      "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" }
    },
    "./server": {
      "import": { "types": "./dist/server/index.d.ts", "default": "./dist/server/index.js" },
      "require": { "types": "./dist/server/index.d.cts", "default": "./dist/server/index.cjs" }
    },
    "./web": {
      "import": { "types": "./dist/web/index.d.ts", "default": "./dist/web/index.js" },
      "require": { "types": "./dist/web/index.d.cts", "default": "./dist/web/index.cjs" }
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": ["kitbase", "feature-flags", "flags", "openfeature"],
  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/scr2em/kitbase-sdk.git", "directory": "flags/typescript" },
  "peerDependencies": {
    "@openfeature/server-sdk": "^1.0.0",
    "@openfeature/web-sdk": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@openfeature/server-sdk": { "optional": true },
    "@openfeature/web-sdk": { "optional": true }
  },
  "devDependencies": {
    "@openfeature/server-sdk": "^1.15.0",
    "@openfeature/web-sdk": "^1.2.0",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": { "node": ">=18" }
}
