{
  "name": "@kitbase/sdk",
  "version": "0.1.1",
  "description": "Official Kitbase SDK for TypeScript and JavaScript",
  "type": "module",
  "exports": {
    "./events": {
      "import": {
        "types": "./dist/events/index.d.ts",
        "default": "./dist/events/index.js"
      },
      "require": {
        "types": "./dist/events/index.d.cts",
        "default": "./dist/events/index.cjs"
      }
    },
    "./changelogs": {
      "import": {
        "types": "./dist/changelogs/index.d.ts",
        "default": "./dist/changelogs/index.js"
      },
      "require": {
        "types": "./dist/changelogs/index.d.cts",
        "default": "./dist/changelogs/index.cjs"
      }
    },
    "./flags": {
      "import": {
        "types": "./dist/flags/index.d.ts",
        "default": "./dist/flags/index.js"
      },
      "require": {
        "types": "./dist/flags/index.d.cts",
        "default": "./dist/flags/index.cjs"
      }
    },
    "./flags/server": {
      "import": {
        "types": "./dist/flags/server/index.d.ts",
        "default": "./dist/flags/server/index.js"
      },
      "require": {
        "types": "./dist/flags/server/index.d.cts",
        "default": "./dist/flags/server/index.cjs"
      }
    },
    "./flags/web": {
      "import": {
        "types": "./dist/flags/web/index.d.ts",
        "default": "./dist/flags/web/index.js"
      },
      "require": {
        "types": "./dist/flags/web/index.d.cts",
        "default": "./dist/flags/web/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "kitbase",
    "logging",
    "events",
    "analytics",
    "tracking",
    "changelogs",
    "feature-flags",
    "openfeature"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scr2em/kitbase-sdk.git",
    "directory": "packages/typescript/sdk"
  },
  "bugs": {
    "url": "https://github.com/scr2em/kitbase-sdk/issues"
  },
  "homepage": "https://github.com/scr2em/kitbase-sdk/tree/main/packages/typescript/sdk#readme",
  "devDependencies": {
    "@openfeature/server-sdk": "^1.15.0",
    "@openfeature/web-sdk": "^1.2.0",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@openfeature/server-sdk": "^1.0.0",
    "@openfeature/web-sdk": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@openfeature/server-sdk": {
      "optional": true
    },
    "@openfeature/web-sdk": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dexie": "^4.0.10",
    "uuid": "^13.0.0"
  }
}
