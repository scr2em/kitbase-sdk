{
  "name": "@kitbase/react",
  "version": "0.1.0",
  "description": "Official Kitbase React SDK - Hooks and providers for React applications",
  "type": "module",
  "exports": {
    "./flags": {
      "import": {
        "types": "./dist/flags.d.ts",
        "default": "./dist/flags.js"
      },
      "require": {
        "types": "./dist/flags.d.cts",
        "default": "./dist/flags.cjs"
      }
    },
    "./events": {
      "import": {
        "types": "./dist/events.d.ts",
        "default": "./dist/events.js"
      },
      "require": {
        "types": "./dist/events.d.cts",
        "default": "./dist/events.cjs"
      }
    },
    "./changelogs": {
      "import": {
        "types": "./dist/changelogs.d.ts",
        "default": "./dist/changelogs.js"
      },
      "require": {
        "types": "./dist/changelogs.d.cts",
        "default": "./dist/changelogs.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "kitbase",
    "react",
    "hooks",
    "feature-flags",
    "events",
    "changelogs",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scr2em/kitbase-sdk.git",
    "directory": "packages/typescript/react"
  },
  "bugs": {
    "url": "https://github.com/scr2em/kitbase-sdk/issues"
  },
  "homepage": "https://github.com/scr2em/kitbase-sdk/tree/main/packages/typescript/react#readme",
  "dependencies": {
    "@kitbase/sdk": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "jsdom": "^23.0.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
