{
  "name": "@kitbase/events",
  "version": "0.1.0",
  "description": "Kitbase Events SDK - Track events and logs in your application",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "kitbase",
    "logging",
    "events",
    "analytics",
    "tracking"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitbase/kitbase-sdk.git",
    "directory": "packages/typescript/events"
  },
  "bugs": {
    "url": "https://github.com/kitbase/kitbase-sdk/issues"
  },
  "homepage": "https://github.com/kitbase/kitbase-sdk/tree/main/packages/typescript/events#readme",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
