{
  "name": "@kitbase/ionic",
  "version": "0.1.0",
  "description": "Kitbase CLI for Ionic - Build and upload your Ionic apps to Kitbase",
  "type": "module",
  "bin": {
    "kitbase-ionic": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "kitbase",
    "ionic",
    "capacitor",
    "mobile",
    "build",
    "deploy",
    "cli",
    "ci-cd"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitbase/kitbase-sdk.git",
    "directory": "packages/typescript/ionic"
  },
  "bugs": {
    "url": "https://github.com/kitbase/kitbase-sdk/issues"
  },
  "homepage": "https://github.com/kitbase/kitbase-sdk/tree/main/packages/typescript/ionic#readme",
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}

